<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hbm2ddl.auto">update</property>
		<property name="show_sql">true</property>
		<mapping class="com.xworkz.springcm.temple.dto.AppPropertyDTO"></mapping>
		<mapping class="com.xworkz.springcm.temple.entity.AppPropertyENTITY"></mapping>
		<mapping class="com.xworkz.springcm.temple.dto.RegistrationDTO"></mapping>
		<mapping class="com.xworkz.springcm.temple.entity.PersonalInfoENTITY"></mapping>
		<mapping class="com.xworkz.springcm.temple.entity.VisitingDetailsENTITY"></mapping>
	</session-factory>
</hibernate-configuration>